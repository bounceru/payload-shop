{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "types": ["expo", "node"],
    "target": "esnext"
  },
  "include": ["**/*.tsx", "**/*.ts"]
}
